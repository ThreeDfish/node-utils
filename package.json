    "@coolgk/tmp": "^2.0.2",
    "@types/amqplib": "^0.5.6",
    "@types/bcrypt-nodejs": "0.0.30",
    "@types/busboy": "^0.2.3",
    "@types/csv-parse": "^1.1.11",
    "@types/csv-stringify": "^1.4.2",
    "@types/mongodb": "^3",
    "@types/node": "^9",
    "@types/phantom": "^3.2.4",
    "@types/redis": "^2.8.5",
    "@types/request": "^2.0.13",
    "@types/uuid": "^3.4.3",
    "amqplib": "^0.5.2",
    "bcrypt-nodejs": "0.0.3",
    "busboy": "^0.2.14",
    "csv-parse": "^2.0.0",
    "csv-stringify": "^2.0.1",
    "emailjs": "^1.0.12",
    "mime-types": "^2.1.17",
    "phantom": "^4.0.12",
    "redis": "^2.8.0",
    "request": "^2.83.0",
    "tmp": "0.0.33",
    "uuid": "^3.2.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "chalk": "^2.3.0",
    "coveralls": "^3.0.0",
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-header": "^1.8.9",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-typescript": "^3.2.4",
    "js-yaml": "^3.10.0",
    "jsdoc-to-markdown": "^3.0.4",
    "mocha": "^4.1.0",
    "nsp": "^3.1.0",
    "nyc": "^11.4.1",
    "pre-commit": "^1.2.2",
    "retire": "^1.4.0",
    "sinon": "^4.2.1",
    "snyk": "^1.69.3",
    "source-map-support": "^0.5.3",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2"
  },
  "pre-commit": {
    "silent": false,
    "run": [
      "tslint",
      "test:precommit"
    ]
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "license": "MIT"
}
