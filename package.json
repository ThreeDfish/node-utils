{
    "name": "@threedfish/utils",
    "version": "3.1.5",
    "author": "Daniel Gong <daniel.k.gong@gmail.com>",
    "contributors": [
        {
            "name": "Dan Willett",
            "email": "dan@3dfish.net"
        }
    ],
    "homepage": "https://github.com/threedfish/node-utils",
    "bugs": {
        "url": "https://github.com/threedfish/node-utils/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/threedfish/node-utils.git"
    },
    "description": "javascript, typescript utility and wrapper functions and classes: array, string, base64, ampq, bcrypt, cache, captcha, csv, email, jwt, number, pdf, tmp, token, unit conversion, url params, session, form data, google sign in, facebook sign in",
    "keywords": [
        "array",
        "string",
        "base64",
        "ampq",
        "cache",
        "captcha",
        "csv",
        "email",
        "jwt",
        "number",
        "typescript",
        "tmp",
        "token",
        "unit conversion",
        "url params",
        "session",
        "express",
        "formdata",
        "middleware",
        "multipart",
        "x-www-form-urlencoded",
        "form",
        "google sign in",
        "google login",
        "facebook sign in",
        "facebook log in"
    ],
    "type": "module",
    "sourceType": "module",
    "scripts": {
        "pretest": "tsc --project tsconfig.dev.json",
        "test": "export $(cat .env | xargs) && nyc mocha test",
        "test:quick": "export $(cat .env | xargs) && mocha --require source-map-support/register test",
        "test:one": "export $(cat .env | xargs) && mocha --require source-map-support/register",
        "test:precommit": "export $(cat .env | xargs) && tsc --project tsconfig.json && nyc --reporter text-summary mocha test --reporter min",
        "updatecheck": "find $PWD/packages/* -type d -print0 | xargs -0 -n1 -t -I {} sh -c 'cd {} && npm outdated'",
        "tslint": "tslint --exclude src/test.ts src/**/*",
        "tsc": "tsc --project tsconfig.json",
        "tsc:watch": "tsc --project tsconfig.debug.json -w --pretty",
        "tsc:dev": "tsc --project tsconfig.dev.json",
        "coverage": "nyc report --reporter=text-lcov | coveralls",
        "debug": "export $(cat .env | xargs) && nodemon --watch ./dist --inspect=0.0.0.0:9229 --nolazy ./dist/test.js",
        "package": "gulp package",
        "publish": "gulp publish",
        "audit": "gulp audit"
    },
    "dependencies": {
        "@threedfish/array": "^3.1.5",
        "@threedfish/base64": "^3.1.5",
        "@threedfish/cache": "^3.1.5",
        "@threedfish/jwt": "^3.1.5",
        "@threedfish/queue": "^3.1.5",
        "@threedfish/string": "^3.1.5",
        "@threedfish/tmp": "^3.1.5",
        "@threedfish/token": "^3.1.5",
        "@types/request": "^2.47.0",
        "@types/request-promise-native": "^1.0.14",
        "amqplib": "^0.10.3",
        "busboy": "^1.6.0",
        "cookie": "^0.5.0",
        "csv-parse": "^5.3.6",
        "csv-stringify": "^6.3.0",
        "emailjs": "^4.0.1",
        "google-auth-library": "^8.7.0",
        "got": "^12.6.0",
        "mime-types": "^2.1.35",
        "redis": "^4.6.5",
        "request": "^2.88.2",
        "request-promise-native": "^0.0.0",
        "tmp": "0.2.1",
        "uuid": "^9.0.0"
    },
    "devDependencies": {
        "@types/amqplib": "^0.10.1",
        "@types/busboy": "^1.5.0",
        "@types/cookie": "^0.5.1",
        "@types/csv-parse": "^1.2.2",
        "@types/csv-stringify": "^3.1.0",
        "@types/mime-types": "^2.1.1",
        "@types/mongodb": "^4.0.7",
        "@types/node": "^18.15.11",
        "@types/redis": "^2.8.6",
        "@types/tmp": "0.2.3",
        "@types/uuid": "^9.0.1",
        "chai": "^4.3.7",
        "chai-as-promised": "^7.1.1",
        "chalk": "^5.2.0",
        "coveralls-next": "^4.2.0",
        "del": "^7.0.0",
        "eslint": "^8.38.0",
        "eslint-config-google": "^0.14.0",
        "express": "^4.18.0",
        "js-yaml": "^4.1.0",
        "jsdoc-to-markdown": "^8.0.0",
        "mocha": "^10.2.0",
        "nyc": "^14.1.1",
        "pre-commit": "^1.2.2",
        "retire": "^3.2.3",
        "sinon": "^15.0.3",
        "snyk": "^1.1140.0",
        "source-map-support": "^0.5.21",
        "tslint": "^6.1.3",
        "typescript": "^5.0.4"
    },
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "license": "MIT",
    "workspaces": [
        "packages/array",
        "packages/base64",
        "packages/cache",
        "packages/jwt",
        "packages/queue",
        "packages/string",
        "packages/tmp",
        "packages/token",
        "packages/amqp",
        "packages/formdata",
        "packages/csv"
    ]
}
